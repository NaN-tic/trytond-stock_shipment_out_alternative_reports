<tryton>
    <data>
        <record model="ir.ui.view" id="shipment_out_view_form">
            <field name="model">stock.shipment.out</field>
            <field name="inherit" ref="stock.shipment_out_view_form"/>
            <field name="name">shipment_out_form</field>
        </record>

        <record model="ir.model.button" id="print_delivery_note_button">
            <field name="name">print_delivery_note_button</field>
            <field name="string">Delivery Note</field>
            <field name="model" search="[('model', '=', 'stock.shipment.out')]"/>
        </record>

        <record model="ir.action.report" id="report_delivery">
            <field name="name">Shipment Out</field>
            <field name="model">stock.shipment.out</field>
            <field name="report_name">stock.shipment.out.delivery</field>
            <field name="single" eval="True"/>
        </record>
    </data>
</tryton>
